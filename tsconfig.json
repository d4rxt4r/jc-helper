{
  "compilerOptions": {
    "module": "ES2022",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*"
      ],
      "./Types/*": [
        "src/Types/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
}